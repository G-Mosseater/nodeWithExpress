<section class="pagination">
    <% if (hasPreviousPage) { %>
        <a href="?page=<%= previousPage %>">Previous</a>
        <% } %>

            <span>Page <%= currentPage %> of <%= lastPage %></span>

            <% if (hasNextPage) { %>
                <a href="?page=<%= nextPage %>">Next</a>
                <% } %>
</section>